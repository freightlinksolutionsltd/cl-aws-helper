{
  "name": "cl-aws-helper",
  "version": "1.0.0",
  "description": "AWS helper for CL Server and lambdas",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "lint": "npx eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier . --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freightlinksolutionsltd/cl-aws-helper.git"
  },
  "keywords": [
    "aws"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/freightlinksolutionsltd/cl-aws-helper/issues"
  },
  "homepage": "https://github.com/freightlinksolutionsltd/cl-aws-helper#readme",
  "devDependencies": {
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.1",
    "pretty-quick": "^3.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.1304.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^2.10.0"
  }
}
