{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": 0,
    "no-empty": 0,
    "no-irregular-whitespace": 0,
    "no-plusplus": 0,
    "no-use-before-define": 0,
    "no-underscore-dangle": 0,
    "semi": [2, "always"]
  }
}
